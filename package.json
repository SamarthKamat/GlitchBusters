{
  "name": "food-waste-reduction-network",
  "version": "1.0.0",
  "description": "A web application connecting businesses with excess food to local charities",
  "main": "server.js",
  "scripts": {
    "start": "node server/server.js",
    "server": "nodemon backend/server.js",
    "client": "cd frontend && npm run dev",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "install-client": "cd frontend && npm install",
    "build": "cd frontend && npm run build",
    "heroku-postbuild": "npm run install-client && npm run build"
  },
  "keywords": [
    "food waste",
    "charity",
    "sustainability",
    "mern"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@date-io/date-fns": "^3.2.1",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@heroicons/react": "^2.2.0",
    "@mui/icons-material": "^5.14.16",
    "@mui/material": "^6.4.8",
    "@mui/x-date-pickers": "^7.28.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "framer-motion": "^10.16.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "socket.io": "^4.7.2",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.21",
    "concurrently": "^8.2.1",
    "nodemon": "^3.0.1",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.0.17",
    "vite": "^6.2.3"
  }
}
